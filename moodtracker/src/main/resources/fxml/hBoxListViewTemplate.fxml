<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="33.0" prefWidth="509.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="lbDate" prefHeight="17.0" prefWidth="96.0" text="Label">
         <HBox.margin>
            <Insets top="5.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="lbName" prefHeight="17.0" prefWidth="100.0" text="Label">
         <HBox.margin>
            <Insets top="5.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="lbDescription" prefHeight="17.0" prefWidth="212.0" text="Label">
         <HBox.margin>
            <Insets top="5.0" />
         </HBox.margin>
      </Label>
      <Button fx:id="btnEdit" mnemonicParsing="false" text="Edit">
         <HBox.margin>
            <Insets right="5.0" />
         </HBox.margin>
      </Button>
      <Button fx:id="btnDelete" cancelButton="true" mnemonicParsing="false" text="Delete" />
   </children>
   <padding>
      <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
   </padding>
   <stylesheets>
      <URL value="@../styles/stylesheet.css" />
      <URL value="@../styles/colors.css" />
   </stylesheets>
</HBox>
